<div class="user_container ">
    <h1> All Shipments </h1>

    <br>

    <% @shipments.each do |shipment| %>
        <h3><%= link_to "#{shipment.customer}", shipment_path(shipment) %></h3>
        <p><%= link_to "#{shipment.service.name} - #{shipment.service.carrier.name}", service_path(shipment.service) %></p> 
        <p>Shipped on: <%= shipment.ship_date.strftime("%m/%d/%Y") %></p><br>
    <% end %>


    <br><br>
</div>